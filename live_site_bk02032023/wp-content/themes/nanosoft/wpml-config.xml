    <shortcode>
      <tag>iconlist_item</tag>
      <attributes>
        <attribute>content</attribute>
        <attribute>class</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>iconlist</tag>
      <attributes>
        <attribute>class</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>iconbox</tag>
      <attributes>
        <attribute>title</attribute>
        <attribute>content</attribute>
        <attribute>link</attribute>
        <attribute>text</attribute>
