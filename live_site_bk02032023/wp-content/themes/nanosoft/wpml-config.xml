  <custom-types>
    <custom-type translate="1">nproject</custom-type>
  </custom-types>
  
  <taxonomies>
    <taxonomy translate="1">nproject-category</taxonomy>
    <taxonomy translate="1">nproject-tag</taxonomy>
  </taxonomies>
  
  <admin-texts>
    <key name="theme_mods_glb">
      <key name="mods">
        <key name="header__topbar__text"/>
        <key name="footer__copyright__content"/>
      </key>
      <key name="header__topbar__text"/>
      <key name="footer__copyright__content"/>
      <key name="project__related__title"/>
    </key>
  </admin-texts>
</wpml-config>