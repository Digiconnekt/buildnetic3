      </attributes>
    </shortcode>
    <shortcode>
      <tag>scrollslider</tag>
    </shortcode>
    <shortcode>
      <tag>scrollslider_item</tag>
      <attributes>
        <attribute>title</attribute>
        <attribute>content</attribute>
        <attribute>link</attribute>
        <attribute>class</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>member</tag>
      <attributes>
        <attribute>name</attribute>
        <attribute>subtitle</attribute>
        <attribute>content</attribute>
        <attribute>facebook</attribute>
        <attribute>twitter</attribute>
        <attribute>linkedin</attribute>
        <attribute>google</attribute>
        <attribute>class</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>testimonial</tag>
      <attributes>
