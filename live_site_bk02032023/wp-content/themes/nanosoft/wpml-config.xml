      <tag>maps</tag>
      <attributes>
        <attribute>address</attribute>
        <attribute>height</attribute>
        <attribute>content</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>elements_carousel</tag>
      <attributes>
        <attribute>class</attribute>
        <attribute>autoplay_speed</attribute>
        <attribute>slide_speed</attribute>
        <attribute>pagination_speed</attribute>
        <attribute>rewind_speed</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>projects</tag>
      <attributes>
        <attribute>widget_title</attribute>
        <attribute>categories</attribute>
        <attribute>tags</attribute>
        <attribute>limit</attribute>
        <attribute>offset</attribute>
        <attribute>thumbnail_size</attribute>
        <attribute>readmore_text</attribute>
        <attribute>class</attribute>
      </attributes>
    </shortcode>
